    "cors": "^2.8.5",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^7.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^6.13.0",
    "stripe": "^20.1.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "typescript": "^5.4.0"
  },
  "private": true
